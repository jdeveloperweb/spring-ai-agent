# Spring AI Agent - Environment Variables Example
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=jdbc:postgresql://localhost:5432/agent_db
DATABASE_USER=agent_user
DATABASE_PASS=agent_pass

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4o-mini

# Spring Profile
SPRING_PROFILES_ACTIVE=dev

# Server Configuration
SERVER_PORT=8080

# API Keys for Development
# Format: key:tenant:name
# Example: dev-token-123:tenant1:Development Key
DEV_API_KEY=dev-token
DEV_TENANT=default

# Production API Key (change in production!)
PROD_API_KEY=prod-token
PROD_TENANT=production

# Logging Level
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_APP=DEBUG

# RAG Configuration
RAG_SIMILARITY_THRESHOLD=0.75
RAG_TOP_K=6

# Execution Configuration
AGENT_EXECUTION_TIMEOUT=PT5M
AGENT_MAX_CONCURRENT_TASKS=10

# Redis Configuration (if using caching)
REDIS_HOST=localhost
REDIS_PORT=6379

# Vector Store Configuration
VECTOR_STORE_INDEX_TYPE=HNSW
VECTOR_STORE_DISTANCE_TYPE=COSINE_DISTANCE
VECTOR_STORE_DIMENSIONS=1536
